<div class="dealsFrom">
  <%= form_for :deal do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />

  <%= f.label :description %>:
  <%= f.text_field :description %><br />

  <%= f.label :start_time %>:
  <%= f.text_field :start_time %><br />

  <%= f.label :end_time %>:
  <%= f.text_field :end_time %><br />

  <%= f.submit %>
<% end %>
</div>

